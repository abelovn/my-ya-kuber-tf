[defaults]
inventory = ./inventory/inventory.ini
roles_path = ./roles
host_key_checking = no

[ssh_connection]
ssh_args = -o ProxyCommand="ssh -p 22 -W %h:%p -q ubuntu@${master_nat_ip_address} -o StrictHostKeyChecking=no" -o ForwardAgent=yes -o ControlMaster=auto -o ControlPersist=15m -o UserKnownHostsFile=/dev/null -o StrictHostKeyChecking=no
pipelining = yes
